datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
  default  = true
}

generator client {
  provider = "prisma-client-js"
}

model User {
  // essentials
  id        String   @id @default(uuid())
  intId     Int      @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  firstName String? 
}

